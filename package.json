{
  "name": "template-fullstack-firebase",
  "version": "1.0.0",
  "description": "Fullstack monorepo with GCP, Firebase and React",
  "main": "index.js",
  "scripts": {
    "dev:ui": "concurrently -k \"npm:start:ui\" \"npm:emulators\"",
    "dev:functions": "concurrently -k \"npm:start:functions\" \"npm:emulators\"",
    "start:ui": "npm run dev --workspace=@changeme/ui-webapp",
    "start:functions": "npm run dev --workspace=@changeme/api-functions",
    "emulators": "firebase emulators:start --import=./firebase-data --export-on-exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RodyGL/template-fullstack-firebase.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RodyGL/template-fullstack-firebase/issues"
  },
  "homepage": "https://github.com/RodyGL/template-fullstack-firebase#readme",
  "workspaces": [
    "./packages/*"
  ],
  "devDependencies": {
    "@types/node": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vitejs/plugin-react": "^1.1.4",
    "builtin-modules": "^3.2.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "vite": "^2.7.13"
  }
}
